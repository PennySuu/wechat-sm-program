  content: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYCAYAAADgdz34AAAAhElEQVRIS+2VwQ2AMAwD7U3YBEZgBEZhFEaBTToCGxil6gNeSR99EUtVP26inGKVGCz21Je0ArhJntF3vQ12K0yy3hFlA5dSIvoLIkkLgNmdFzCfKZLkyxJft0jSBMCOp60ZDs8IoJAsuaYuqUSUiL4EJA3/MmvaLaEu/GboWtNo0bfvASvYXRnB3JcFAAAAAElFTkSuQmCC')
}
