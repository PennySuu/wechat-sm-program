.container {
  color: #999;
  padding: 40rpx 0 40rpx 35rpx;
}

.wrap {
  padding-right: 35rpx;
}

.item {
  width: 100%;
}

.search {
  background-color: #f9f9f9;
  padding: 20rpx;
  border-radius: 5rpx;
}

.search::before {
  content: url('data:image/png;base64,');
  vertical-align: middle;
}

.section {
  padding: 20rpx;
}

.section_title {
  padding: 35rpx 0;
}

.section_star_title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.section_star_star::before {
  content: url('data:image/png;base64,');
  vertical-align: middle;
  margin-right: 10rpx;
}

.section_star_close::after {
  content: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA4AAAAOCAYAAAAfSC3RAAAAoUlEQVQ4T63SbRHCMAyA4XcOkIAEJGwOJgEcgAMk4AAk4GA4AClI2L07woUxbh9Hf7W9PE2bpgBWwBp4MG0Y+yyADXAHdsBlxBrbAJXQsQXOIzjQwQQBM66AWy+z6Aoc41YZBj4BZXpzoDrXoQ/FOdC1mT6Qm0MwY+df6O/QClsEM02+quhXcfav93anzfkOv0jcNcncBnjjJS0nLoWLmrwF0Zssj8OfWUwAAAAASUVORK5CYII=')'';
  vertical-align: middle;
  margin-left: 10rpx;
}

.scroll-view_H {
  width: 100%;
  white-space: nowrap;
}

.scroll-view-item_H {
  display: inline-block;
  margin-right: 2%;
  width: 95%;
}

.star {
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding: 35rpx 0;
  border: 1px solid #e6e6e6;
}

.scroll-view-item_H .right {
  width: 230rpx;
  height: 230rpx;
  border-radius: 50%;
  overflow: hidden;
}

.scroll-view-item_H .right image {
  width: 100%;
  height: 100%;
}

.scroll-view-item_H .left {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  margin-right: 100rpx;
}

.scroll-view-item_H .info {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.scroll-view-item_H .left .name {
  color: #3c3c3c;
  font-size: 50rpx;
}

.scroll-view-item_H .left button {
  color: #cb9730;
  border-radius: 0;
  border-color: #cb9730;
}

.section_history_title::before {
  content: url('data:image/png;base64,');
  vertical-align: middle;
  margin-right: 10rpx;
}

.section_history_picture {
  height: 320rpx;
}

.section_history_picture .picture {
  width: 100%;
  height: 100%;
}

.section_history_rhesis {
  padding: 35rpx 0 5rpx;
  color: #3c3c3c;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1rpx solid #e6e6e6;
}

.section_history_rhesis .date {
  font-size: 48rpx;
}

.section_history_rhesis .rhesis {
  margin-left: 5rpx;
  flex: 1 1 auto;
}

.section_history_rhesis::after {
  content: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAh0lEQVQ4T72TyxGDMAxERU1qJOkAOqMUKMAepRNSwO5mfODCECD2EJ21T6tfZ43RNertHoC7jyml/oq7XQfuLjObADwjYjkCHQGKLgAMERHfIGcAk7SQLE6mPcgpYBVJGnLO4xbyF8AbwKOqBUkvkn3VECXNJEvlqjW2HdKVC1xz7vmFXxx8ABYbVxEMGABhAAAAAElFTkSuQmCC');
}

.section_birth {
  margin-top: 65rpx;
}

.section_birth_item {
  padding: 40rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #e6e6e6;
}

.section_birth_item:last-child {
  border-bottom: none;
}

.section_birth_head {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 25rpx;
}

.section_birth_head image {
  width: 100%;
  height: 100%;
}

.section_birth_body {
  flex: 1 1 auto;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.section_birth_name {
  color: #3c3c3c;
  font-size: 30rpx;
}

.section_birth_day {
  display: flex;
  align-items: flex-end;
  color: #ff393a;
  font-size: 34rpx;
}

.section_birth_day view:last-child {
  font-size: 24rpx;
}

.section_birth_age {
  font-size: 24rpx;
}

.add {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #ff393a;
  position: fixed;
  bottom: 35rpx;
  right: 45rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

